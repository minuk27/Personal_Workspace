package egovframework.msa.minuk.messageDto;

import lombok.Getter;

@Getter
public class MailListDto {
	String userId;
	int mailIndex;
	String date;
	String from;
	String title;
	
	public MailListDto(String userId, int mailIndex, String date, String from, String title) {
		this.userId = userId;
		this.mailIndex = mailIndex;
		this.date = date;
		this.from = from;
		this.title = title;
	}
}